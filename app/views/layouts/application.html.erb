<!DOCTYPE>
<html ng-app="tdlist">

<head>
  <title>TDlist <%= yield(:title) %></title>
  <%= stylesheet_link_tag    "application", media: "all"%>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>

<body>
	<div class="content"> 
		<% flash.each do |key,value| %>
	      <div class="flash"><%= value %></div>
	    <% end %>
	    <%= yield %>
	</div>
</body>

</html>